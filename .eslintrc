{
  "parserOptions": {
    "ecmaVersion": 2019,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "strict": ["error", "never"],
    "valid-typeof": "warn",
    "no-unsafe-negation": "warn",
    "no-unused-vars": "warn",
    "no-unexpected-multiline": "warn",
    "no-undef": "warn"
  },
  // "extends": ["airbnb", "prettier"],
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
